<!doctype html>

<html>

<head>
  <title>Voice Chat</title>
  <meta charset="utf-8">

</head>

<body>
  <div class="container">
    <div class="infobox">
      <p>Enter a username: <input id="name" type="text" maxlength="12" required autocomplete="username"
          inputmode="verbatim" placeholder="Username">
        <input type="button" name="login" value="Log in" onclick="connect()"></p>
    </div>
    <ul class="userlistbox"></ul>
    <!--<div class="chatbox"></div>-->


    <audio id="received_audio" autoplay></audio>

    <audio id="local_audio" autoplay muted></audio>

    <button id="hangup-button" onclick="hangUpCall();" role="button" disabled>
      Hang Up
    </button>

    <a id="download" href="#" style="display: none;">Download</a>

    <div class="empty-container"></div>
  </div>

  <script src="https://code.jquery.com/jquery-3.5.1.min.js"
    integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
  <script type="text/javascript" src="/assets/chatclient.js"></script>
  <script type="text/javascript" src="/assets/adapter.js"></script>
</body>

</html>